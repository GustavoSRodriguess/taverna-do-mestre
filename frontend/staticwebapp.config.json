{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/assets/*",
            "/favicon.ico",
            "/*.css",
            "/*.js",
            "/*.png",
            "/*.jpg",
            "/*.jpeg",
            "/*.gif",
            "/*.svg",
            "/*.webp",
            "/*.ico",
            "/*.woff",
            "/*.woff2",
            "/*.ttf",
            "/*.json"
        ]
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html"
        }
    }
}