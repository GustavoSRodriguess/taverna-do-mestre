{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/assets/*",
            "/*.js",
            "/*.css",
            "/*.json",
            "/*.png",
            "/*.jpg",
            "/*.jpeg",
            "/*.gif",
            "/*.svg",
            "/*.ico",
            "/*.woff",
            "/*.woff2",
            "/*.ttf",
            "/*.webp",
            "/favicon.ico"
        ]
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    }
}